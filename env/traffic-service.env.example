# Traffic Service Environment Configuration Template
# Copy this file to env/traffic-service.env and fill in the actual values

NODE_ENV=development
PORT=4004

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Kafka Configuration
KAFKA_BROKERS=localhost:9092
KAFKA_CLIENT_ID=tms-traffic-service

# Traffic API Configuration (if using external APIs)
# TRAFFIC_API_URL=https://api.traffic.com
# TRAFFIC_API_KEY=your-traffic-api-key-here

# Traffic Processing Configuration
TRAFFIC_UPDATE_INTERVAL=30000
TRAFFIC_CACHE_TTL=300
