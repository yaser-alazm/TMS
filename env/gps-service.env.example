# GPS Service Environment Configuration Template
# Copy this file to env/gps-service.env and fill in the actual values

NODE_ENV=development
PORT=4003

# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5434/tms_gps

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Kafka Configuration
KAFKA_BROKERS=localhost:9092
KAFKA_CLIENT_ID=tms-gps-service

# GPS Configuration
GPS_UPDATE_INTERVAL=5000
GPS_ACCURACY_THRESHOLD=10
